<!--<h1>Admin::Customers#index</h1>-->
<div>
  <div class='row'>
    <div>
      <h2>会員一覧</h2>
      <tr>
        <th>会員ID</th>
        <th>氏名</th>
        <th>メールアドレス</th>
        <th>ステータス</th>
      </tr>
      <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.id %></td>
        <td><%= customer.last_name %></td>
        <td><%= customer.email %></td>
        <td><%= "ステータス" %></td>
      </tr>
      <% end %>
      
      <%= paginate customers %>
    </div>
  </div>
</div>
