<div class = "row" >
 <div class = "col-xs-12" >
  <h1>配送先編集</h1><br>
   <%= form_with model: @delivery_address do |f| %>
    <h1>郵便番号</h1>
     <%= f.text_field :post_code %>
    <h1>住所</h1>
     <%= f.text_field :address %> 
    <h1>宛先</h1>
     <%= f.text_field :addressee %>
     <%= f.submit '変更内容を保存' %>
   <% end %>
 </div>
</div>